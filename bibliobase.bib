  Issn =         {0378-4290},
  Journal =      {Field Crops Research},
  Keywords =     {Zinc},
  Number =       {1--2},
  Pages =        {27 - 40},
  Title =        {Agronomic approaches for improving the micronutrient
                  density in edible portions of field crops},
  Volume =       60,
