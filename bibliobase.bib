  Note =         {Genome studies and Molecular Genetics, edited by
                  Juliette de Meaux and Maarten Koornneef / Plant
                  Biotechnology, edited by Andy Greenland and Jan
                  Leach},
  Number =       2,
  Pages =        {166 - 170},
  Title =        {Biofortified crops to alleviate micronutrient
                  malnutrition},
